<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="com.sun.enterprise.jst.server.sunappsrv.register.splashHandlers.InteractiveSplashHandler"
            id="com.sun.enterprise.jst.server.sunappsrv.register.splashHandlers.interactive">
      </splashHandler>
      <splashHandlerProductBinding
            productId="org.eclipse.platform.ide"
            splashId="com.sun.enterprise.jst.server.sunappsrv.register.splashHandlers.interactive">
      </splashHandlerProductBinding>
   </extension>
          <extension
                id="glassfishBundle"
                name="%productName"
                point="org.eclipse.core.runtime.products"> 
      <product name="%productName" application="org.eclipse.ui.ide.workbench" description="%productBlurb"> 
          <property name="preferenceCustomization" value="plugin_customization.ini"/> 
      </product> 
   </extension>
</plugin>
