<?xml version='1.0' encoding='UTF-8'?>
<project default="all">

	<target name="all">
		<antcall target="extractSampleDB" />
	</target>

    <target
        name="extractSampleDB">
		<!--echo message="In extract sample db, glassfishv3_dir is ${glassfishv3_dir}"/>
		<echo message="calling mkdir for ${glassfishv3_dir}/databases"/-->   	
    	<mkdir dir="${glassfishv3_dir}/databases"/>
        <unzip
            src="${basedir}/../lib/derbysampledb.zip"
            dest="${glassfishv3_dir}/databases/sun-appserv-samples" />
    </target>
</project>