<?xml version='1.0' encoding='UTF-8'?>
<project default="update.domain">

	<target name="update.domain">
		<echo>${domain.dir}   ${previousDir}     ${newDir}</echo>
		<replace dir="${domain.dir}">
			<include name="**" />
			<replacefilter token="${previousDir}" value="${newDir}" />
		</replace>
	</target>
</project>